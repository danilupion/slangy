{
  "name": "@slangy/react",
  "version": "2.3.0",
  "author": {
    "name": "Dani LupiÃ³n",
    "email": "danilupion@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "if ! [[ $NOCLEAN = \"1\" ]]; then rm -rf ./dist; fi && tsc",
    "build:watch": "export NOCLEAN=1 && nodemon --watch src -e ts,json --exec 'yarn build'",
    "build:check": "tsc -p tsconfig.json --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "lint:fix": "eslint -c .eslintrc.json 'src/**/*.ts' --fix",
    "lint": "eslint -c .eslintrc.json 'src/**/*.ts'",
    "prettify": "prettier --write  \"./src/**/*.{ts,json}\""
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=18.0.0 <19.0.0",
    "swr": ">=2.0.0 <3.0.0",
    "zustand": ">=4.0.0 <5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "swr": "^2.2.4",
    "typescript": "^5.3.2",
    "zustand": "^4.4.6"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn prettify",
      "yarn lint:fix --max-warnings=0",
      "yarn test"
    ]
  }
}
