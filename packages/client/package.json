{
  "name": "@danilupion/turbo-client",
  "version": "1.1.0",
  "author": {
    "name": "Dani Lupi√≥n",
    "email": "danilupion@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "if ! [[ $NOCLEAN = \"1\" ]]; then rm -rf ./dist; fi && tsc",
    "build:watch": "export NOCLEAN=1 && nodemon --watch src -e ts,json --exec 'yarn build'",
    "build:check": "tsc -p tsconfig.json --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "lint:fix": "eslint -c .eslintrc.json 'src/**/*.ts' --fix",
    "lint": "eslint -c .eslintrc.json 'src/**/*.ts'",
    "prettify": "prettier --write  \"./src/**/*.{ts,json}\""
  },
  "dependencies": {
    "@danilupion/turbo-common": "1.1.0",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.12",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn prettify",
      "yarn lint:fix --max-warnings=0",
      "yarn test"
    ]
  }
}
